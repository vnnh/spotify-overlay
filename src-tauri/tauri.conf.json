{
	"package": {
		"productName": "spotify-overlay",
		"version": "0.1.0"
	},
	"build": {
		"distDir": "../out",
		"devPath": "http://localhost:8081",
		"beforeDevCommand": "npm run web-dev",
		"beforeBuildCommand": "npm run web-build"
	},
	"tauri": {
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "com.tauri.dev",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"resources": [],
			"externalBin": [],
			"copyright": "",
			"category": "DeveloperTool",
			"shortDescription": "",
			"longDescription": "",
			"deb": {
				"depends": [],
				"useBootstrapper": false
			},
			"macOS": {
				"frameworks": [],
				"minimumSystemVersion": "",
				"useBootstrapper": false,
				"exceptionDomain": "",
				"signingIdentity": null,
				"entitlements": null
			},
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"updater": {
			"active": false
		},
		"allowlist": {
			"http": {
				"all": true
			}
		},
		"windows": [
			{
				"title": "spotify-overlay",
				"width": 290,
				"height": 60,
				"resizable": true,
				"fullscreen": false,
				"alwaysOnTop": true,
				"decorations": false,
				"transparent": true
			}
		],
		"systemTray": {
			"iconPath": "icons/icon.png",
			"iconAsTemplate": true
		},
		"security": {
			"csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
		}
	}
}
